{
  "name": "rt-vue-theme",
  "description": "WordPress theme using Rest API and Vue.js by rtCamp",
  "version": "1.0.0",
  "author": "rtCamp",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint": "eslint .",
    "phpcs": "phpcs --standard=WordPress-Core ./functions.php",
    "ci": "concurrently \"npm run build\" \"npm run lint\""
  },
  "dependencies": {
    "axios": "^0.16.1",
    "progressive-image": "^1.0.1",
    "vue": "^2.2.1",
    "vue-resource": "^1.2.1",
    "vue-router": "^2.3.0",
    "vuex": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.0.0",
    "babel-preset-latest": "^6.0.0",
    "concurrently": "^3.4.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.19.0",
    "eslint-plugin-html": "^2.0.1",
    "file-loader": "^0.9.0",
    "foundation-sites": "^6.3.1",
    "fs-extra": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-combine-mq": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^2.1.2",
    "gulp-watch": "^4.3.11",
    "jquery.mmenu": "^5.7.8",
    "path": "^0.12.7",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
